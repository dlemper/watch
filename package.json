{
  "name": "@vue-reactivity/watch",
  "version": "0.1.6",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.global.js",
  "jsdelivr": "dist/index.global.js",
  "repository": "https://github.com/antfu/vue-reactivity-watch.git",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "prepare": "npm run build",
    "build": "tsup src/index.ts --format cjs,esm,iife --dts",
    "dev": "npm run build -- --watch",
    "test": "c8 ava",
    "release": "npx bumpp --tag --commit --push && pnpm publish --access public"
  },
  "peerDependencies": {
    "@vue/reactivity": ">=3.0.0-rc.1",
    "@vue/shared": ">=3.0.0-rc.1"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.3.4",
    "@types/node": "^14.18.9",
    "@vue/reactivity": "^3.2.29",
    "@vue/shared": "^3.2.29",
    "ava": "^3.15.0",
    "c8": "^7.11.0",
    "esbuild-register": "^1.2.1",
    "eslint": "^7.32.0",
    "esm": "^3.2.25",
    "tsup": "^3.12.1",
    "typescript": "^4.5.5"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "esm",
      "esbuild-register"
    ]
  },
  "dependencies": {
    "esbuild": "^0.14.13"
  }
}
